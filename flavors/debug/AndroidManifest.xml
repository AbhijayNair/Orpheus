<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andrew.apollo">

    <application>
        <!-- Local Content Provider-->
        <provider
            android:authorities="org.opensilk.music.provider.debug"
            android:name="com.andrew.apollo.provider.MusicProvider"
            android:multiprocess="true"
            android:exported="false"/>

        <provider
            android:name="org.opensilk.music.artwork.ArtworkProvider"
            android:authorities="org.opensilk.music.artwork.provider.debug"
            android:enabled="true"
            android:exported="false"
            android:grantUriPermissions="true"/>

    </application>

</manifest>